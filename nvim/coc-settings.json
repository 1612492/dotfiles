{
  "coc.preferences.useQuickfixForLocations": true,
  "diagnostic.errorSign": " ",
  "diagnostic.hintSign": " ",
  "diagnostic.infoSign": " ",
  "diagnostic.warningSign": " ",
  "floatFactory.floatConfig": {
    "border": true,
    "rounded": true
  },
  "languageserver": {
    "lua": {
      "command": "lua-language-server",
      "args": ["--stdio"],
      "filetypes": ["lua"],
      "rootPatterns": [".luarc.json"]
    },
    "solidity": {
      "command": "nomicfoundation-solidity-language-server",
      "args": ["--stdio"],
      "filetypes": ["solidity"],
      "rootPatterns": ["hardhat.config.ts"]
    },
    "tact": {
      "command": "tact-extracted-ls",
      "args": ["--stdio"],
      "filetypes": ["tact"],
      "ignoredRootPaths": ["~"]
    },
    "tailwindcss": {
      "command": "tailwindcss-language-server",
      "args": ["--stdio"],
      "filetypes": ["html", "css", "javascriptreact", "typescriptreact"],
      "rootPatterns": ["tailwind.config.ts", "tailwind.config.js"]
    }
  },
  "suggest.defaultSortMethod": "alphabetical",
  "suggest.floatConfig": {
    "border": true,
    "rounded": true
  },
  "suggest.noselect": true,
  "snippets.extends": {
    "typescript": ["javascript", "txt"],
    "typescriptreact": ["javascript", "txt"]
  },
  "snippets.textmateSnippetsRoots": ["~/.config/nvim/snippets"],
  "snippets.ultisnips.pythonPrompt": false
}
