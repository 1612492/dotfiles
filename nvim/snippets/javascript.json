{
  "console.log": {
    "prefix": "cl",
    "body": "console.log($0)"
  },
  "console.error": {
    "prefix": "ce",
    "body": "console.error($0)"
  },
  "switch": {
    "prefix": "switch",
    "body": [
      "switch ($1) {",
      "\tcase $2:",
      "\t\t$0",
      "\t\tbreak;",
      "\tdefault:",
      "\t\tbreak;",
      "}"
    ]
  },
  "tryCatch": {
    "prefix": "trycatch",
    "body": ["try {", "\t$0", "} catch (${1:error}) {", "\t", "}"]
  },
  "import": {
    "prefix": "imp",
    "body": ["import $2 from '$1'"]
  },
  "useState": {
    "prefix": "useStateSnippet",
    "body": ["const [$1, set$2] = useState($0)"]
  },
  "react": {
    "prefix": "rfce",
    "body": [
      "function $1() {",
      "\t\treturn (",
      "\t\t\t<div>$0</div>",
      "\t\t)",
      "}",
      "",
      "export default $2"
    ]
  }
}
